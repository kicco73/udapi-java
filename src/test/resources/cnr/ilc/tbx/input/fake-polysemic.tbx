<tbx xmlns="urn:iso:std:iso:30042:ed-2" type="TBX-Basic" style="dct" xml:lang="en" xmlns:min="http://www.tbxinfo.net/ns/min" xmlns:basic="http://www.tbxinfo.net/ns/basic">
	<tbxHeader>
		<fileDesc>
			<sourceDesc>
				<p>A sample of the TBX-Basic dialect</p>
			</sourceDesc>
		</fileDesc>
